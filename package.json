    "@remix-run/vercel": "^1.6.0",
    "classnames": "^2.3.1",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.1",
    "gsap": "^3.10.4",
    "prismjs": "^1.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reading-time": "^1.5.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.0",
    "@remix-run/eslint-config": "^1.6.0",
    "@remix-run/serve": "^1.6.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.0",
